{
  "name": "annextube-frontend",
  "version": "0.1.0",
  "description": "Client-side web interface for browsing YouTube archives",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:utils": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.js",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.svelte",
    "format": "prettier --write 'src/**/*.{ts,svelte,css}'"
  },
  "dependencies": {
    "date-fns": "^3.0.0",
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-schema-to-typescript": "^13.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
